  "GoogleSsoService": {
    "RedirectUri": "https://authorization-dev.Benraz.com/v1/auth/google-callback"
  },
  "TokenValidation": {
    "Audience": "DEV - Benraz Authorization Server"
  },
  "BenrazAuthGateway": {
    "BaseUrl": "https://authorization-dev.Benraz.com"
  },
  "BenrazServices": {
    "BaseUrl": "https://servicestest.Benraz.com/"
